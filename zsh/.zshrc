# Alias a quick http server
alias server="php -S localhost:8000"

# Quick folder
alias ..='cd ../'
alias ...='cd ../../'

# Docker
alias dcu='docker-compose up'
alias dcd='docker-compose down'

# Easy clear
alias c='clear'

# Git
alias gs='git status'
alias gr='git reset --hard'
alias gc='git commit .'
alias push='git push'
alias pull='git pull'